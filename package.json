{
  "name": "tape-webpack-boilerplate",
  "version": "1.0.0",
  "description": "a sample setup of tape for unit testing and bundling with webpack",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --watch",
    "build": "webpack",
    "test": "tape -r @babel/register ./test/**/*.js | tap-spec",
    "test:watch": "nodemon --watch src -x \"npm run test\""
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/puiutucutu/tape-webpack-boilerplate.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/puiutucutu/tape-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/puiutucutu/tape-webpack-boilerplate#readme"
}
